{
    "name": "naowas/laravel-full-calendar",
    "description": "Laravel helper for FullCalendar.io. Fork of https://github.com/maddhatter/laravel-fullcalendar",
    "authors": [
        {
            "name": "Naowas Morshed Eimon",
            "email": "naowas.morshed@gmail.com",
            "homepage": "https://naowas.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.2",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "laravel/helpers": "^1.0"
    },
    "license": "MIT",
    "homepage": "https://github.com/nelkasovic/laravel-full-calendar",
    "support": {
        "email": "naowas.morshed@gmail.com",
        "issues": "https://github.com/naowas/laravel-full-calendar/issues",
        "wiki": "https://github.com/naowas/laravel-full-calendar/wiki",
        "source": "https://github.com/naowas/laravel-full-calendar",
        "docs": "https://github.com/naowas/laravel-full-calendar"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "LaravelFullCalendar\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelFullCalendar\\FullCalendarServiceProvider"
            ],
            "aliases": {
                "Calendar": "LaravelFullCalendar\\Facades\\Calendar"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
